<% appointment = serialize_resource(audit_activity.appointment) %>
<li class="list-group-item js-activity t-activity" id="activity-<%= audit_activity.id %>" data-appointment="<%= appointment %>">
  <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
  <b><%= audit_activity.user_name %></b> changed the <%= audit_activity.message %> <em class="badge"><%= time_ago_in_words(audit_activity.created_at) %> ago</em>
</li>
